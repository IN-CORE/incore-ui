		"react-redux": "~7.1.0",
		"react-router": "~3.2.0",
		"react-router-redux": "~4.0.8",
		"redux": "~4.0.1",
		"redux-thunk": "~2.3.0",
		"universal-cookie": "^4.0.2",
		"vis": "4.18.1",
		"whatwg-fetch": "2.0.2"
	},
